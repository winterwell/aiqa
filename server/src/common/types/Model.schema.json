{
  "$ref": "#/definitions/Model",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Model": {
      "additionalProperties": false,
      "description": "E.g. connection details for ChatGPT 4.1, Claude 3.5 Sonnet, etc.",
      "properties": {
        "api_key": {
          "type": "string"
        },
        "created": {
          "format": "date-time",
          "type": "string"
        },
        "description": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "name": {
          "description": "e.g. gpt-4o This is the name as used by the provider's API",
          "type": "string"
        },
        "organisation": {
          "type": "string"
        },
        "provider": {
          "enum": [
            "openai",
            "anthropic",
            "google",
            "azure",
            "bedrock",
            "other"
          ],
          "type": "string"
        },
        "updated": {
          "format": "date-time",
          "type": "string"
        },
        "version": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "organisation",
        "provider",
        "name",
        "api_key",
        "created",
        "updated"
      ],
      "type": "object"
    }
  }
}
