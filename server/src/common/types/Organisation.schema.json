{
  "$ref": "#/definitions/Organisation",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "Organisation": {
      "additionalProperties": false,
      "properties": {
        "created": {
          "format": "date-time",
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "members": {
          "description": "User ids of members of the organisation. Must contain the current user's id.",
          "items": {
            "type": "string"
          },
          "type": "array"
        },
        "name": {
          "type": "string"
        },
        "rate_limit_per_hour": {
          "type": "number"
        },
        "retention_period_days": {
          "type": "number"
        },
        "updated": {
          "format": "date-time",
          "type": "string"
        }
      },
      "required": [
        "id",
        "name",
        "members",
        "created",
        "updated"
      ],
      "type": "object"
    }
  }
}
