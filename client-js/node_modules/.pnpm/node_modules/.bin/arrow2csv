#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/daniel/aiqa/client-js/node_modules/.pnpm/apache-arrow@21.1.0/node_modules/apache-arrow/bin/node_modules:/home/daniel/aiqa/client-js/node_modules/.pnpm/apache-arrow@21.1.0/node_modules/apache-arrow/node_modules:/home/daniel/aiqa/client-js/node_modules/.pnpm/apache-arrow@21.1.0/node_modules:/home/daniel/aiqa/client-js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/daniel/aiqa/client-js/node_modules/.pnpm/apache-arrow@21.1.0/node_modules/apache-arrow/bin/node_modules:/home/daniel/aiqa/client-js/node_modules/.pnpm/apache-arrow@21.1.0/node_modules/apache-arrow/node_modules:/home/daniel/aiqa/client-js/node_modules/.pnpm/apache-arrow@21.1.0/node_modules:/home/daniel/aiqa/client-js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../apache-arrow/bin/arrow2csv.js" "$@"
else
  exec node  "$basedir/../apache-arrow/bin/arrow2csv.js" "$@"
fi
